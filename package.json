{
  "name": "vllm-docker-api",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "tsx server/src/index.ts",
    "build": "tsc -p server/tsconfig.json",
    "start": "node server/dist/index.js",
    "docs:api": "node -e \"import('./server/dist/openapi.js').then(m=>{console.log(JSON.stringify(m.generateOpenApiDocument()))})\" > docs/openapi.json",
    "docs:code": "typedoc --options typedoc.json"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "express": "^4.19.2",
    "zod": "^3.25.76",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "@types/express": "^4.17.21",
    "@types/node": "^22.7.4",
    "swagger-ui-express": "^5.0.1",
    "@asteasolutions/zod-to-openapi": "^7.1.1"
  },
  "devDependencies": {
    "typedoc": "^0.26.10"
  }
}